<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
		
		.seekBox {
			padding: 10px;
			float: left;
		}
	</ui:style>
	<g:HTMLPanel>
		<div class="{style.seekBox}">
			Seek List:
			<br />
			<g:ListBox ui:field="seekBox" visibleItemCount="10" width="200px" />
		</div>

		<div class="{style.seekBox}">
			Match List:
			<br />
			<g:ListBox ui:field="matchBox" visibleItemCount="10" width="200px" />
		</div>

		<g:Button ui:field="autoUpdateButton"  />
	</g:HTMLPanel>
</ui:UiBinder>
